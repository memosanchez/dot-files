[user]
  name = Guillermo A. Sanchez
  email = memo.sanchez@gmail.com
[core]
  excludesfile = ~/.gitignore
  pager=cat
[color]
  ui = auto
[github]
	user = memosanchez
[alias]
  b = branch
  c = commit
  co = checkout
  st = status -s
  lg = log --format='%Cred%h%Creset %s %Cgreen(%cr) %C(blue)<%an>%Creset%C(yellow)%d%Creset' --no-merges
[push]
  default = simple
